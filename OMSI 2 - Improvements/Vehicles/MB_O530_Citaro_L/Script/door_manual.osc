{macro:DoorAft_Open}
	
  	(L.L.PAX_Exit0_Req)
	(L.L.PAX_Exit1_Req) ||
	(L.L.bremse_halte) &&
   {if}
      (L.L.doorTarget_23) !
      (L.L.bremse_halte_sw) &&
      {if}
	(T.L.ev_doortriggeropen_2)
         1 (S.L.doorTarget_23)
         (C.L.door_druckluft)
         {if}
         (L.L.bremse_p_Tank04) 100000 - 0.96 * 100000 + (S.L.bremse_p_Tank04)
         {endif}
      {endif}
      0 (S.L.doorAftLastOpen)      
   {endif}

	(L.L.PAX_Exit2_Req)
	(L.L.PAX_Exit3_Req) ||
	(L.L.bremse_halte) &&
   {if}
      (L.L.doorTarget_45) !
